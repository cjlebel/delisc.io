{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "WebApiSettings": {
        "ApiKey": "[SUPER-SECRET-API-KEY]",
        "BaseUrl": "http://localhost:5000"
    },
    "ApiKey": "[SUPER-SECRET-API-KEY]",
    "MongoDbOptions": {
        "ConnectionString": "mongodb://mongo:[MONGO-PASSWORD]@[MONGO-HOST]:[MONGO-PORT]",
        "DatabaseName": "deliscio"
    },
    "LinksQueueSettings": {
        "Host": "rabbitmq://[RABBIT-HOST]:[RABBIT-PORT]/",
        "Username": "[USERNAME]",
        "Password": "[PASSWORD]",
        "QueueName": "submitted-links",
        "MaxConcurrentCalls": 1,
        "MaxDequeueCount": 5,
        "VisibilityTimeout": 60,
        "MaxRetryCount": 5,
        "BannedHosts": [
            "127.0.0.1",
            "192.168.",
            "172.20.",
            "localhost",

            "calendar.",
            "docs.google.com",
            "drive.google",
            "login.",
            "mail.",
            "meet.google.com",

            "amazon.ca",
            "amazon.com",
            "ebay.ca",
            "ebay.com",
            "kijiji.ca",
            "kijiji.com"
        ]
    }
}
